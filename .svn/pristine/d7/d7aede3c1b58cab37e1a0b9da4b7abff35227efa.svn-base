/*样式重置*/
body,h1,h2,h3,h4,h5,h6,p,dl,dd,ul,ol,form,input,textarea,th,td,select{margin:0;padding:0;}
em{font-style:normal;}
li{list-style:none;}
a{text-decoration:none;}
img{border:none;vertical-align:top;}
table{border-collapse:collapse;}
input,textarea{outline:none;}
textarea{resize:none;overflow:auto;}
html,body{font-size:12px;font-family:"微软雅黑";width: 100%;height: 100%;}
/*end 样式重置*/

/*top*/
#top {min-width: 800px;width: 100%;height: 80px;background-color: #EEE;zoom: 1;}
#top :after{content: "";clear: both;}
#logo {float: left; margin-left: 20px; }
#logo a {display: block;height: 80px;width: 210px;background-image: url(../img/logo.png);background-size:100%;outline: none;}
#message {float: right;margin-right: 20px; }
#message a{padding-left:50px;}
#message a,label {display: inline;color: #14191e;font-size: 16px;line-height: 30px;height: 30px;}
#message a:hover{color: rgba(0,170,170,.85);}
/*end top*/

/*left*/
#left_side{position: absolute;min-width: 300px;width: 200px;height: 100%;left: 50px;top: 50px;}
#left_side ul li{height: 60px;border-bottom: 1px solid #EEE;width: 160px;margin: 0 auto;}
#left_side ul a{display: block;height: 60px;font-size: 16px;line-height: 60px;text-align: center;color: #14191e;}
#left_side ul a:link{color: #14191e;}
#left_side ul a:visited{color: #14191e;}
#left_side ul a:hover{color: rgba(0,170,170,.85);}
#left_side ul a:active{color: rgba(0,170,170,.85);}
/*end left*/

/*public*/
.clear{zoom:1;}
.clear:after{content:"";display: block;clear:both;}

.a{color: #14191e;cursor: pointer;}
.a:link{color: #14191e;}
.a:visited{color: #14191e;}
.a:hover{color: rgba(0,170,170,.85);}
.a:active{color: rgba(0,170,170,.85);}

.ipt,.select{color: #787d82;line-height: 14px;font-size: 14px;margin-right: 20px;width: 100px;padding: 4px 8px;border: 1px solid #787d82;border-radius: 5px;}
.ipt:focus,.select:focus{border-color: rgba(0,170,170,.85);box-shadow: inset 0 1px 1px rgba(0,0,0,.075),0 0 3px rgba(0,170,170,.85);}

.btn{display:inline-block; margin: 0 10px;padding: 2px 8px;border-radius: 5px;color: #787d82;background-color: rgba(0,170,170,.85);border: 1px solid #787d82;cursor: pointer;vertical-align: middle;}
.btn:link{color: #787d82;}
.btn:visited{color: #787d82;}
.btn:hover{color: #14191e;}
.btn:active{color: #14191e;}
/*end public*/

.sys_main{width: 100%;min-width: 500px;height: 100%;}
.sys_wrap,.sys_content {min-width: 500px;padding: 20px;border-bottom: 1px solid #EEE;}
.sys_wrap p{display: block;color: #787d82;line-height: 16px;font-size: 16px;}
.sys_content table{color: #787d82;line-height: 14px;font-size: 14px;border: 1px solid #787d82;}
.sys_content th{background-color: #EEE;}
.sys_content th,td{display: table-cell;text-align: center;padding:15px;border-bottom: 1px solid #787d82;border-right: 1px solid #787d82;}
.sys_content tr:hover{background-color: #EEE;}
.sys_select{margin: 0 30px;float: right;}

/*Info*/
.label-name{float: left;width: 80px;height: 40px;text-align: right;color: #787d82;line-height: 40px;font-size: 14px;}
.info_left{float: left;width: 390px;margin-left: 8px;color: #787d82;}
.info_clear {width: 480px;margin: 10px auto 20px;}
.info_clear input{width: 300px;height: 20px;padding: 9px 20px 9px 40px;}
/*end Info*/
/*page */
.pagin{position:relative;margin-top:10px;padding:0 12px;}
.pagin .blue{color:#056dae;font-style:normal;}
.pagin .paginList{position:absolute;right:12px;top:0;}
.pagin .paginList .paginItem{float:left;}
.pagin .paginList .paginItem a{float:left;width:31px;height:28px;border:1px solid #DDD; text-align:center;line-height:30px;border-left:none;color:#3399d5;}
.pagin .paginList .paginItem:first-child a{border-left:1px solid #DDD;}
.pagin .paginList .paginItem:first-child a{border-bottom-left-radius:5px;border-top-left-radius:5px;}
.pagin .paginList .paginItem:last-child a{border-bottom-right-radius:5px;border-top-right-radius:5px;}
.pagin .paginList .paginItem.current,.pagin .paginList .paginItem.current a{background:#f5f5f5; cursor:default;color:#737373;}
.pagin .paginList .paginItem:hover{background:#f5f5f5;}
.pagin .paginList .paginItem.more,.pagin .paginList .paginItem.more a:hover{ cursor:default;}
.pagin .paginList .paginItem.more:hover{background:#FFF;}
.pagin .paginList .paginItem.more a{color:#737373;}
.pagepre{background:url(../images/pre.gif) no-repeat center center; width:31px; height:28px;}
.pagenxt{background:url(../images/next.gif) no-repeat center center; width:31px; height:28px;}
