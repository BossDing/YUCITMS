package com.yucitms.orm.netLesson;

import java.util.Date;
import java.util.HashSet;
import java.util.Set;

/**
 * 精品课程
 * @author qiangzi
 *
 */
public class ExcellentCourse implements java.io.Serializable{
	private Integer id;
	private String teachingThought;//教学思想
	private String teachingContent;//教学内容
	private String courseParsing;//课程解析
	private String teachingTeam;//教学团队
	private String teachingAim;//教学目的
	private String teachingRequire;//教学目的
	private String practice;//实习
	private String work;//作业
	private String scheduling;//进度安排
	private Date createDate;//创建时间
	private Set<LessionBook> books = new HashSet<LessionBook>();//对应的书籍
	private Set<ReferenceData> datas =  new HashSet<ReferenceData>();//参考资料
	private Set<ExpandApplication> applications =  new HashSet<ExpandApplication>();//扩展应用
	private Set<ExperimentCase> experimentCases =  new HashSet<ExperimentCase>();//实训案例
	private Set<DifficultyGuidance> guidances =  new HashSet<DifficultyGuidance>();//重难点
	private Set<ExcellentTeacher> teachers= new HashSet<ExcellentTeacher>();//教师团队
	private Set<ExcellentVideo> videos = new HashSet<ExcellentVideo>();//视频
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public String getTeachingThought() {
		return teachingThought;
	}
	public void setTeachingThought(String teachingThought) {
		this.teachingThought = teachingThought;
	}
	public String getTeachingContent() {
		return teachingContent;
	}
	public void setTeachingContent(String teachingContent) {
		this.teachingContent = teachingContent;
	}
	public String getCourseParsing() {
		return courseParsing;
	}
	public void setCourseParsing(String courseParsing) {
		this.courseParsing = courseParsing;
	}
	public String getTeachingTeam() {
		return teachingTeam;
	}
	public void setTeachingTeam(String teachingTeam) {
		this.teachingTeam = teachingTeam;
	}
	public String getTeachingAim() {
		return teachingAim;
	}
	public void setTeachingAim(String teachingAim) {
		this.teachingAim = teachingAim;
	}
	public String getTeachingRequire() {
		return teachingRequire;
	}
	public void setTeachingRequire(String teachingRequire) {
		this.teachingRequire = teachingRequire;
	}
	public String getPractice() {
		return practice;
	}
	public void setPractice(String practice) {
		this.practice = practice;
	}
	public String getWork() {
		return work;
	}
	public void setWork(String work) {
		this.work = work;
	}
	public String getScheduling() {
		return scheduling;
	}
	public void setScheduling(String scheduling) {
		this.scheduling = scheduling;
	}
	public Date getCreateDate() {
		return createDate;
	}
	public void setCreateDate(Date createDate) {
		this.createDate = createDate;
	}
	public Set<LessionBook> getBooks() {
		return books;
	}
	public void setBooks(Set<LessionBook> books) {
		this.books = books;
	}
	public Set<ReferenceData> getDatas() {
		return datas;
	}
	public void setDatas(Set<ReferenceData> datas) {
		this.datas = datas;
	}
	public Set<ExpandApplication> getApplications() {
		return applications;
	}
	public void setApplications(Set<ExpandApplication> applications) {
		this.applications = applications;
	}
	public Set<ExperimentCase> getExperimentCases() {
		return experimentCases;
	}
	public void setExperimentCases(Set<ExperimentCase> experimentCases) {
		this.experimentCases = experimentCases;
	}
	public Set<DifficultyGuidance> getGuidances() {
		return guidances;
	}
	public void setGuidances(Set<DifficultyGuidance> guidances) {
		this.guidances = guidances;
	}
	public Set<ExcellentTeacher> getTeachers() {
		return teachers;
	}
	public void setTeachers(Set<ExcellentTeacher> teachers) {
		this.teachers = teachers;
	}
	public Set<ExcellentVideo> getVideos() {
		return videos;
	}
	public void setVideos(Set<ExcellentVideo> videos) {
		this.videos = videos;
	}
	
}
