<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="login" namespace="/"  extends="struts-default">
	    <action name="login_*" class="com.yucitms.action.student.LoginAction" method="{1}">
	    <result name="loginpage">/WEB-INF/page/login.jsp</result>
	    </action>
	    <action name="admin_*" class="com.yucitms.action.AdminAction" method="{1}">
	     <result name="left">/WEB-INF/page/admin/left.jsp</result>
	    <result name="top">/WEB-INF/page/admin/top.jsp</result>
	    <result name="main">/WEB-INF/page/admin/sysfirst.jsp</result>
	    <result name="index">/WEB-INF/page/admin/index.jsp</result>
	    </action>
     </package> 
</struts>    	