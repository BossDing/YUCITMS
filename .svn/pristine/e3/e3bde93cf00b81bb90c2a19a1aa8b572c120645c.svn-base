a.button {
	background: transparent url('../images/button/button_right.png')
		no-repeat scroll top right;
	display: block;
	float: left;
	margin-right: 6px;
	padding-right: 20px;
	cursor:pointer;
}

a.button span {
	color: #666666;
	font-size: 12px;
	line-height: 22px;
	padding: 4px 0 5px 18px;
	background: transparent url('../images/button/button_left.png')
		no-repeat;
	display: block;
}

a.button:hover span {
	text-decoration: none;
}
.MM a:link {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
	line-height: 26px;
	color: #666666;
	background-image: url(../images/menu_bg.gif);
	background-repeat: no-repeat;
	list-style-type: none;
	list-style-image: none;
}

