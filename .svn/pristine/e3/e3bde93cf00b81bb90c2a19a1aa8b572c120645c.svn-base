}

.showPage {
	font-family: Arial, Helvetica, sans-serif;
	line-height: normal;
	font-size: 14px;
}

.showPage table {
	float: right;
}

.showPage table td {
	text-align: center;
}

.showPage table td a {
	display: block;
	color: #0054A0;
	font-weight: normal;
	text-decoration: none;
	letter-spacing: 0px;
	border: 1px solid #ccc;
	padding-top: 2px;
	padding-bottom: 2px;
	padding-right: 5px;
	padding-left: 5px;
	float: left;
	margin-top: 5px;
	margin-right: 2px;
	margin-bottom: 5px;
	margin-left: 2px;
	text-align: center;
	height: 18px
}

.showPage table td a:hover {
	color: #F44;
	border: 1px solid #F44;
}

.showPage table td a:active {
	color: #F44;
	border: 1px solid #F44;
}

.showPage table td input {
	border: 1px solid #ccc;
	font: 16px;
	width: 30px;
	padding-top: 0px;
	padding-bottom: 0px;
	min-height: 24px;
}
.active {
	color: white;
	background-color: silver;
}

.from_td_input{
	border:1px #9ECFFE solid;
	width: 200px;
}